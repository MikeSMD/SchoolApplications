<Window x:Class="WPF_projekt.UpravSkolu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_projekt"
        mc:Ignorable="d"
        Title="UpravSkolu" Height="236" Width="527">
    <StackPanel Height="211" VerticalAlignment="Top">
        <DockPanel>
            <Label>nazev:</Label>
            <TextBox Text="{Binding nazev}"/>
        </DockPanel>

        <DockPanel>
            <Label>kraj:</Label>
            <TextBox Text="{Binding kraj}"/>
        </DockPanel>

        <DockPanel>
            <Label>mesto:</Label>
            <TextBox Text="{Binding mesto}"/>
        </DockPanel>

        <DockPanel>
            <Label>ulice:</Label>
            <TextBox Text="{Binding ulice}"/>
        </DockPanel>

        <DockPanel>
            <Label>psc:</Label>
            <TextBox Text="{Binding psc}"/>
        </DockPanel>

        <DockPanel>
            <Label>typ:</Label>
            <TextBox Text="{Binding typ}"/>
        </DockPanel>

        <DockPanel>
            <Label>kontakt:</Label>
            <TextBox Text="{Binding email}"/>
        </DockPanel>

        <Button Click="Ulozit" Content="Uložit"/>

    </StackPanel>
</Window>
